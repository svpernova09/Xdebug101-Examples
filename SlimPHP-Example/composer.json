{
    "name": "jeremykendall/flaming-archer",
    "description": "A photo 365 project application. Requires user to have a Flickr API key",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Kendall",
            "homepage": "http://about.me/jeremykendall",
            "role": "Developer"
        }
    ],
    "keywords": [
        "photography",
        "flickr",
        "photo-a-day",
        "photo 365"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-mcrypt": "*",
        "doctrine/common": "~2.4",
        "guzzle/guzzle": "~3.8",
        "ircmaxell/password-compat": "1.*",
        "jeremykendall/slim-auth": "dev-develop",
        "monolog/monolog": "1.*",
        "pimple/pimple": "2.*",
        "slim/slim": "~2.4",
        "slim/views": "0.1.*",
        "symfony/event-dispatcher": "2.3.*",
        "twig/twig": "1.13.*",
        "wp-cli/php-cli-tools": "~0.9.4",
        "zendframework/zend-authentication": "2.3.*",
        "zendframework/zend-cache": "2.3.*",
        "zendframework/zend-config": "2.3.*",
        "zendframework/zend-feed": "2.3.*",
        "zendframework/zend-inputfilter": "2.3.*",
        "zendframework/zend-paginator": "2.3.*",
        "zendframework/zend-serializer": "2.3.*",
        "zendframework/zend-session": "2.3.*"
    },
    "require-dev": {
        "composer/composer": "1.0.*@dev",
        "mikey179/vfsStream": "1.2.*",
        "mockery/mockery": "0.8.0",
        "jeremykendall/phpctagger": "dev-master",
        "phpunit/phpunit": "4.*",
        "whatthejeff/nyancat-phpunit-resultprinter": "v1.2.0"
    },
    "autoload": {
        "psr-0": {
            "FA": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "FA\\Composer\\Script\\SQLite::prepare"
        ],
        "post-update-cmd": [
            "rm -rf templates/cache/*"
        ]
    }
}
